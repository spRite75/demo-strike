generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model LocalUser {
  id Int @id @default(autoincrement())
  username String @unique
  password String
}

model DemoFile {
  id Int @id @default(autoincrement())
  filepath String @unique
  parsed Boolean @default(false)
  deleted Boolean @default(false)
}
